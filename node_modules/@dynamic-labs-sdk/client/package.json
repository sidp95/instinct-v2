{
  "name": "@dynamic-labs-sdk/client",
  "version": "0.4.0",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/exports/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/exports/index.d.ts",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.cjs.js"
    },
    "./core": {
      "types": "./dist/exports/core.d.ts",
      "import": "./dist/core.esm.js",
      "default": "./dist/core.cjs.js"
    },
    "./waas": {
      "types": "./dist/exports/waas.d.ts",
      "import": "./dist/waas.esm.js",
      "default": "./dist/waas.cjs.js"
    },
    "./waas/core": {
      "types": "./dist/exports/waasCore.d.ts",
      "import": "./dist/waasCore.esm.js",
      "default": "./dist/waasCore.cjs.js"
    }
  },
  "dependencies": {
    "@dynamic-labs-wallet/browser-wallet-client": "0.0.250",
    "@dynamic-labs/sdk-api-core": "0.0.860",
    "@simplewebauthn/browser": "13.1.0",
    "buffer": "6.0.3",
    "eventemitter3": "5.0.1",
    "zod": "4.0.5",
    "@dynamic-labs-sdk/assert-package-version": "0.4.0"
  },
  "devDependencies": {
    "@inrupt/jest-jsdom-polyfills": "4.0.5",
    "vitest": "1.6.1"
  },
  "scripts": {
    "build": "rm -rf dist && tsdown && pnpm exec tsc --emitDeclarationOnly -p ./tsconfig.lib.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test-with-coverage": "vitest run --coverage",
    "check-versions": "pnpm --filter @tools/sync-package-versions run sync --check-only --package-filter client"
  }
}